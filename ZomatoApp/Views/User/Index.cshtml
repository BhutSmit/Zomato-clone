                                </form>

                                <a href="#" class="btn_3 ms-4 mt-0">Read More <img src="~/User/img/icon/left_2.svg" alt=""></a>

                            </div>

                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
