<style>

    ::-webkit-scrollbar {
        width: 8px;
        height: 3px;
    }

    /* Track */
    ::-webkit-scrollbar-track {
        background: #ffffff;
    }

    /* Handle */
    ::-webkit-scrollbar-thumb {
        background: rgba(240, 145, 10, 0.8);
        border-radius: 10px;
    }

        ::-webkit-scrollbar-thumb:horizontal {
            background: #efefef;
            border-radius: 10px;
        }

        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
            background: #555;
        }

    .restaurant-card-container::-webkit-scrollbar {
        width: 5px;
        height: 3px;
    }

    /* Track */
    .restaurant-card-container::-webkit-scrollbar-track {
        background: #efefef;
    }

    /* Handle */
    .restaurant-card-container::-webkit-scrollbar-thumb {
        background: rgba(240, 144, 50, 0.8);

    }

        .restaurant-card-container::-webkit-scrollbar-thumb:horizontal {
            background: #efefef;
            border-radius: 10px;
        }

        /* Handle on hover */
        .restaurant-card-container::-webkit-scrollbar-thumb:hover {
            background: #555;
        }

    .restaurant-card-container{

        overflow-x: scroll;

    }

    .favourate-icon-container{

        width: 30px;
        height: 30px;
        /*position: absolute;*/
        /*top: 0;*/
        /*right: 0;*/
        color: rgba(255, 100, 10, 0.5);
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: transparent;
        border: 2px solid rgba(255, 100, 10, 0.2);
        border-radius: 50%;

    }

    .viewmore-button{

        padding: 10px 10px;
        display: inline-block;
        border: 1px solid rgba(255, 150, 10, 0.8);
        border-radius: 20px;

    }

    .visit-button{

        color: rgba(255, 100, 10, 0.8);
        border: 1px solid rgba(255, 100, 10, 0.5);
        background-color: transparent;
        padding: 8px 20px;
        border-radius: 5px 5px 20px 5px;
        transition: all 0.2s linear;

    }

    .visit-button:hover{

        color: #efefef;
        background-color: rgba(255, 100, 10, 0.6);

    }

    .rating-container {

        color: #efefef;
        background-color: rgba(37, 167, 67, 0.9);
        display: inline-block;
        padding: 5px 10px;
        border-radius: 15px;
        font-size: 10px;
        font-weight: 700;

    }

    .card-container{

        width: 270px;
        height: 130px;
        border: 1px solid rgba(200, 200, 220, 0.9);
        border-radius: 5px 5px 20px 5px;
        background-image: linear-gradient(90deg, rgba(255, 100, 10, 0.2), transparent);
        background-size: 0;
        padding: 20px;
        position: relative;
        box-shadow: 0px 5px 10px 0px  rgba(0, 0, 0, 0.1);
        transition: all 0.2s linear;

    }

    .card-container:hover{
        
        transform: translateY(-20px);
        /*background-image: linear-gradient(90deg, rgba(255, 100, 10, 0.2), transparent);*/
        background-size: 100% 100%;
        border: 1px solid rgba(255, 100, 10, 0.8);
    
    }

    .underline{

        position: relative;

    }

    .underline::after{

        content: "";
        width: 100%;
        height: 2px;
        /*background-color: pink;*/
        background-image: linear-gradient(90deg, yellow, rgba(50, 80, 90, 0.5));
        position: absolute;
        top: 100%;
        left: 0%;
        background-position: -100px;
        transition: all 0.5s linear;
    }

    .card-container:hover .underline::after{

        background-position: 0%;

    }

    .name-container{

        font-size: 13px;
        font-weight: 600;
        position: relative;
        background-image: url("https://images.pexels.com/photos/349610/pexels-photo-349610.jpeg?auto=compress&cs=tinysrgb&w=600");
        background-size: 100% 100%;
        /*background-color: rgba(250, 100, 10, 0.2);*/
        display: inline-block;
        color: rgba(10, 10, 10, 0.5);
        /*color: rgba(255, 250, 240, 0.8);*/
        padding: 5px 10px;
        border-radius: 10px;

    }

    .name-container:after{

        content: "";
        width: 2px;
        height: 30px;
        background-color: rgba(209, 198, 196, 0.5);
        position: absolute;
        top: -85%;
        left: 30%;
        transform: rotate(45deg);

    }

    .name-container:before{

        content: "";
        width: 2px;
        height: 30px;
        background-color: rgba(209, 198, 196, 0.5);
        position: absolute;
        top: -85%;
        right: 30%;
        transform: rotate(-45deg);

    }

    .image-container{

        width: 80px;
        height: 80px;
        position: absolute;
        bottom: -10px;
        right: 10px;

    }

    .restaurant-count{

        width: 160px;
        font-size: 13px;
        color: rgba(100, 50, 80, 0.8);
        margin: 10px 0px 0px 0px;
    }


</style>

